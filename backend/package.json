{
  "name": "petswipe-backend",
  "version": "1.0.0",
  "description": "Express + TypeScript + PostgreSQL backend for PetSwipe, a pet adoption app.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:pets": "ts-node -r tsconfig-paths/register scripts/seedPets.ts",
    "assign:user": "ts-node -r tsconfig-paths/register scripts/assignPetsToUser.ts"
  },
  "keywords": [
    "typescript",
    "express",
    "postgresql",
    "typeorm",
    "aws-sdk",
    "swagger",
    "jsonwebtoken",
    "bcrypt",
    "csv-parser",
    "json2csv",
    "petswipe",
    "petfinder",
    "adoption",
    "animal",
    "shelter",
    "rescue",
    "pets"
  ],
  "author": "Son Nguyen",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.49.8",
    "amqplib": "^0.10.8",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "ioredis": "^5.6.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "serverless-http": "^3.2.0",
    "sharp": "^0.34.2",
    "swagger-jsdoc": "^6.2.8",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/aws-sdk": "^2.7.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
